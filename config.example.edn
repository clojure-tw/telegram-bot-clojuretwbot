{;; token get from bot father
 :token "YOUR-BOT-TOKEN"
 ;; long-polling interval (ms)
 :interval 500
 ;; if message is more than threshold, this bot will send sticker to channel
 :threshold 140
 ;; sticker max mulue (0 ~ 5)
 :sticker-range 5

 ;; feed option for feed-parser
 ;; feed-uri, support atom/rss
 :feed-uri "http://planet.clojure.in/atom.xml"
 ;; file to save
 :feed-archive "resources/archive.edn"
 }
